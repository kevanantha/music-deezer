<%- include('../partials/head')  %>
<%- include('../partials/navbar')  %>
  <div class="container">
    <div class="row">
      <% for (let i = 0; i < obj["Songs"].length; i++){ %>
        <div class="col-sm-4">
          <div class="card" style="margin-bottom: 2rem">
            <img class="card-img-top" src="<%= obj["Songs"][i].picture %>" alt="Card image cap" >
            <div class="card-body">
          <h5 class="card-title"><%= obj["Songs"][i].name %></h5>
          <p class="card-text"><%= obj["Songs"][i].artist  %></p>
          <p class="card-text"><a target="_blank" href="<%= obj["Songs"][i].link%>"> For Full Link</a></p>
          <audio controls>
            <source src="<%= obj["Songs"][i].preview %>" type="audio/mpeg">
          Your browser does not support the audio element.
          </audio>
            <a href="/users/playlist/<%= obj["Songs"][i].id %>" class="btn btn-primary">Delete</a>
            </div>
          </div>
        </div>
      <% } %>
    </div>
  </div>
    
<%- include('../partials/footer')  %>
